{
  "manifest_version": 2,
  "name": "WebRip",
  "version": "3.0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "static/js/bundle.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
